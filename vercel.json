{
  "build": {
    "env": {
      "VERCEL_BUILD_DIR": "scramble" 
    },
    "ignoreBuildResult": "scramble",
    "installCommand": "echo 'Skipping unnecessary npm install'"
  },
  "functions": {
    "api/generate.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/.*",
      "dest": "scramble/index.html"
    }
  ]
}
