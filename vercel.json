{
  "build": {
    "env": {
      "VERCEL_BUILD_DIR": "scramble" 
    }
  },
  "functions": {
    "api/generate.js": {
      "runtime": "nodejs20.x"
    }
  },
  "routes": [
    {
      "src": "/api/generate",
      "dest": "/api/generate.js"
    },
    {
      "src": "/.*",
      "dest": "scramble/index.html"
    }
  ],
  "installCommand": "echo 'No dependencies needed for static client'",
  "config": {
    "ignore-build-results": ["scramble"]
  }
}
