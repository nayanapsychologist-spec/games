{
  "version": 2,
  "builds": [
    { "src": "api/generate.js", "use": "@vercel/node" },
    { "src": "scramble/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/scramble/(.*)", "dest": "/scramble/$1" },
    { "src": "/", "dest": "/scramble/index.html" }
  ]
}
